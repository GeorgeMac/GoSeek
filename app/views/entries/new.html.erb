<%= form_for :entry, url: "/entries" do |e| %>
  <p>
    <%= e.label :title %><br>
    <%= e.text_field :title %>
  </p>
 
  <p>
    <%= e.label :description %><br>
    <%= e.text_area :description %>
  </p>

  <p>
    <%= e.label :url %><br>
    <%= e.text_field :url %>
  </p>
 
  <p>
    <%= e.submit %>
  </p>
<% end %>